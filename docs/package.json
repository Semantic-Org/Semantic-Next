{
  "name": "@semantic-ui/docs",
  "type": "module",
  "version": "0.0.2",
  "scripts": {
    "dev": "wireit",
    "astro:dev": "wireit",
    "astro:start": "wireit",
    "astro:build": "wireit",
    "astro:preview": "wireit",
    "astro": "wireit"
  },
  "wireit": {
    "install": {
      "command": "npm install"
    },
    "link": {
      "command": "npm install && npx link ../",
      "dependencies": ["install"]
    },
    "dev": {
      "command": "astro dev",
      "dependencies": [ "link"],
      "service": true
    },
    "build": {
      "command": "astro build",
      "dependencies": [ "link"]
    },
    "astro:dev": {
      "command": "astro dev",
      "service": true
    },
    "astro:build": {
      "command": "astro build"
    },
    "astro:preview": {
      "command": "astro preview"
    },
    "astro": {
      "command": "astro"
    }
  },
  "peerDependencies": {
    "@lit-labs/ssr": "^3.2.2",
    "lit": "^3.1.2"
  },
  "dependencies": {
    "@astrojs/lit": "^4.0.1",
    "@astrojs/mdx": "^2.3.1",
    "@astrojs/starlight": "^0.21.5",
    "@webcomponents/template-shadowroot": "^0.2.1",
    "astro": "^4.7.0",
    "astro-expressive-code": "^0.35.2",
    "link": "^2.1.0",
    "playground-elements": "^0.18.1",
    "pretty": "^2.0.0",
    "shiki": "^1.3.0"
  }
}
