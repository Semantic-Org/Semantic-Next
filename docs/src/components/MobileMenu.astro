---
const { mobileMenu } = Astro.props;
import MobileMenu from '@components/MobileMenu/MobileMenu.js';
import { getSidebarMenu } from '@helpers/navigation.js';

const menu = mobileMenu || await getSidebarMenu({url: Astro.url.pathname});
---
<MobileMenu
	client:load
  menu={menu}
  activeURL={Astro.url.pathname}
/>
